# Quantisity Capital Environment Variables
# Copy this file to .env and fill in your actual values

# Alpha Vantage API (Premium - 600 calls/minute)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# OpenAI API (for GPT-4 morning analysis)
OPENAI_API_KEY=your_openai_key_here

# Stripe API (for payment processing)
STRIPE_API_KEY=your_stripe_key_here

# Twitter API v2 Credentials
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here
TWITTER_CONSUMER_KEY=your_twitter_consumer_key_here
TWITTER_CONSUMER_SECRET=your_twitter_consumer_secret_here
TWITTER_ACCESS_TOKEN=your_twitter_access_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret_here

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# IBKR Account
IBKR_ACCOUNT=your_ibkr_paper_account_here

# Discord Webhooks
DISCORD_BASIC_WEBHOOK=your_basic_webhook_url_here
DISCORD_PREMIUM_WEBHOOK=your_premium_webhook_url_here